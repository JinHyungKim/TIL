class Main {
  public static void main(String[] args) {
		int num = 123456;
		decToBin(num);
	}

	public static void decToBin(int dec){
		if(dec==1){
			System.out.print(dec);
			return;
		}
		else{
			decToBin(dec/2);
			System.out.print(dec%2);
		}
	}
}	

